class Array{
    public static void main(String[] args) {
        //for each
        int[] arr ={1,2,3,4,5,6};
        System.out.println();
        
        for(int i=0; i<arr.length;i++){
            System.out.println(arr[i]);
        }
        for(int num:arr){
            System.out.println(num);
        }
        
    }
}
