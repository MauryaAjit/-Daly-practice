class parul{
  public static void main(String[]args){
    // multi threading 
    MeraClass my=new MeraClass();
    my.start();
  }
}
class MeraClass extends Thread{
  @Override
   public void run(){
    for(int i=1;i<=10;i++){
      try{
        Thread.sleep(2000);
        
      }
      catch(Exception e){
      System.out.println(e.getMessage());
      }
      System.out.println("Message"+Thread.currentThread());
    }
  }
}
