import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();           // Number of elements
        int[] input = new int[n];       // Input values
        int[] hashTable = new int[n];   // Hash table

        // Initialize hash table with -1
        for (int i = 0; i < n; i++) {
            hashTable[i] = -1;
        }

        // Read input values
        for (int i = 0; i < n; i++) {
            input[i] = sc.nextInt();
        }

        // Insert into hash table
        for (int i = 0; i < n; i++) {
            int value = input[i];
            int index = value % n;

            if (hashTable[index] == -1) {
                hashTable[index] = value;
            } else {
                System.out.println("Collision in hashing element " + index);
                hashTable[index] = value; // Replace as per sample behavior
            }
        }

        // Print hash table
        System.out.println("Hash table");
        for (int i = 0; i < n; i++) {
            System.out.println("arr[" + i + "] = " + hashTable[i]);
        }
    }
}
